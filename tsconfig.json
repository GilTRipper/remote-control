{
  "compilerOptions": {
    "target": "ES2020",
    "types": ["node", "jest"],
    "incremental": true,
    "baseUrl": ".",
    "outDir": "dist",
    "exactOptionalPropertyTypes": true,
    "noImplicitReturns": true,
    "strict": true,
    "module": "node16",
    "moduleResolution": "node16",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "experimentalDecorators": true,
    "lib": [
      "ES2020"
    ],
    "skipLibCheck": true
  },
  
}